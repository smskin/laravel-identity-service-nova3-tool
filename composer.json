{
    "name": "smskin/identity-service-nova3-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "~3.0",
        "smskin/laravel-identity-service": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "SMSkin\\IdentityServiceNova3Tool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SMSkin\\IdentityServiceNova3Tool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
